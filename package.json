{
  "name": "miid",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "node scripts/dev.js",
    "dev:desktop": "node scripts/dev-desktop.js",
    "dev:desktop:hot": "node scripts/dev-desktop-hot.js",
    "start:menubar": "electron apps/menubar/main.js",
    "start:service": "node scripts/dev-service.js",
    "start:service:backend": "node apps/service-backend/server.js",
    "start:service:frontend": "node apps/service-frontend/server.js",
    "start:gateway": "node apps/gateway/server.js",
    "start:wallet": "node apps/wallet/server.js",
    "start:frontend": "node apps/service-frontend/server.js",
    "start:backend": "node apps/service-backend/server.js",
    "demo:flow": "bash scripts/demo-flow.sh",
    "test:request": "bash scripts/test-auth-signup.sh",
    "test:finalize": "bash scripts/test-finalize.sh",
    "test:approval": "bash scripts/test-approval.sh",
    "test:e2e": "npm run -s test:approval && npm run -s test:finalize",
    "test:all": "node tests/run-all-tests.js",
    "test:login": "node tests/test-login-flow.js",
    "test:deny": "node tests/test-deny-flow.js",
    "test:revoke": "node tests/test-revoke-flow.js",
    "build:mac": "electron-packager . MiID --platform=darwin --arch=arm64 --overwrite --out=dist --app-bundle-id=com.miid.desktop --app-version=0.1.0 --prune=true --ignore='^/(data|docs|api|db|\\.agent|\\.serena|dist)'"
  },
  "dependencies": {
    "eventsource": "^4.1.0",
    "express": "^5.2.1"
  },
  "devDependencies": {
    "@electron/packager": "^19.0.5",
    "electron": "^40.4.1",
    "oh-my-ag": "^1.21.1"
  }
}
